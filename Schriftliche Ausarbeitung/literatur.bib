@book{balzert2014,
    author = {Balzert, Helmut},
    title = {Java: Objektorientiert programmieren: Vom objektorientierten Analysemodell bis zum objektorientierten Programm},
    year = {2014},
    publisher = {W3L AG},
    address = {Dortmund},
    
    whose = {Luca},
    mynote = {\\Dieses Buch war für den Erfolg unseres Projektes essentiell. Durch bestimmte objektorientierte Designmethoden konnten wir das Projekt erst sinnvoll strukturieren und uns so schließlich auf die Entwicklung der Simulation und der Implementierung der genetischen Algorithmen fokussieren. Auch wenn dieses Buch auf den ersten Blick nichts mit unserem Themengebiet zu tun hat, haben wir auf bestimmte \mbox{Designmethoden} zurückgegriffen und waren nur so in der Lage, die Simulation und den genetischen Algorithmus zu entwickeln.}
}

@book{gerdes2004,
    author = {Gerdes, Ingrid and Klawonn, Frank and Kruse, Rudolf},
    title = {Evolutionäre Algorithmen: Genetische Algorithmen - Strategien Und Optimierungsverfahren - Beispielanwendungen},
    year = {2004},
    publisher = {Friedr. Vierweg & Sohn Verlag},
    address = {Wiesbaden},
    
    whose = {Luca},
    mynote = {\\Diese Quelle konnte mir einen sehr tiefen und gründlichen Einblick in die genetischen Algorithmen geben. Ein Problem dieses Buches war, dass die Mathematik auf Universitätsniveau beschrieben und so teilweise schwierig nachzuvollziehen war. Jedoch war dieses Buch meine wichtigste Quelle zum Verständnis von genetischen Algorithmen. Zusätzlich konnten viele vorgestellte Designmethoden implementiert beziehungsweise abgewandelt eingesetzt werden.}
}

@book{jacobson2015,
    author = {Jacobson, Lee and Kanber, Burak},
    title = {Genetic Algorithms in Java Basics},
    year = {2015},
    publisher = {Apress},
    address = {New York},
    
    whose = {Amon},
    mynote = {\\Dieses Buch bietet zwei große Vorteile; Erstens wird die Idee eines genetischen Algorithmus, die in \cite{gerdes2004} sehr ausführlich beschrieben wird, sinnvoll und verkürzt zusammengefasst. Weiterhin enthält diese Quelle Code-Beispiele verschiedener genetischer Algorithmen in Java. Damit bot das Buch eine hervorragende Grundlage für die Implementierung unseres eigenen genetischen Algorithmus.}
}

